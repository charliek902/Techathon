<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.techathon.HelloController">
   <children>
      <VBox prefHeight="305.0" prefWidth="417.0">
         <children>
            <Pane fx:id="header" prefHeight="35.0" prefWidth="477.0">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="15.0">
                     <children>
                      <Button mnemonicParsing="false" text="Home" />
                        <Button mnemonicParsing="false" text="Learn" />
                        <Button mnemonicParsing="false" text="Quiz" />
                        <Button mnemonicParsing="false" text="Calculators" />
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
               </children>
            </Pane>
            <Pane fx:id="body" style="-fx-background-color: rgb(140,193,193);">
               <children>
                  <VBox alignment="CENTER" fillWidth="false">
                     <children>
                        <Label text="World of Finance" textFill="WHITE">
                           <font>
                              <Font name="Bodoni MT" size="34.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </children>
            </Pane>
            <Pane fx:id="footer" prefHeight="61.0" prefWidth="417.0">
               <children>
                  <HBox alignment="CENTER" fillHeight="false" prefHeight="69.0" prefWidth="417.0" spacing="20.0" style="-fx-background-color: #6e8d63;">
                     <children>
                        <Label text="Copyright @ WOF 2025" textFill="WHITE" wrapText="true">
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </Label>
                        <Label text="WOF is a trademark under the World of Finance group." textFill="WHITE" wrapText="true">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</AnchorPane>
